platform :ios, '12.0'

#use_frameworks!

target 'JiaJiaRong-iOS' do

    # 网络请求
  pod 'AFNetworking', '~> 3.2.1'
  
  # 自动布局
  pod 'Masonry', '1.1.0'
  
  # 工具库
  pod 'YYKit', '~> 1.0.9'
  pod 'MJExtension'
  
  # 加载提示
  pod 'MBProgressHUD', '1.2.0'
  
  # 下拉刷新
  pod 'MJRefresh'
  
  # 键盘管理
  pod 'IQKeyboardManager'

  # 动画 - 交互效果
  pod 'pop', '~> 1.0'

  # 图片加载
  pod 'SDWebImage', '~> 5.0'
  

end 
